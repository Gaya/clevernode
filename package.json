{
  "name": "clevernode",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "~6.5.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node node_modules/wintersmith/bin/wintersmith build && npm run optimize",
    "cleanscss": "find ./build -name *.scss -print0 | xargs -0 rm",
    "cssnano": "node node_modules/cssnano-cli/cmd.js build/assets/css/style.css build/assets/css/style.css",
    "imagemin": "imagemin build/assets/images/* --out-dir=build/assets/images/",
    "install": "npm run build",
    "optimize": "npm run cssnano && npm run imagemin",
    "preview": "node node_modules/wintersmith/bin/wintersmith preview",
    "start": "node server.js"
  },
  "author": "Gaya Kessler",
  "license": "ISC",
  "dependencies": {
    "cssnano-cli": "^1.0.5",
    "imagemin-cli": "^3.0.0",
    "koa": "^1.2.4",
    "koa-static": "^2.0.0",
    "wintersmith": "^2.3.3",
    "wintersmith-nunjucks": "^1.1.0",
    "wintersmith-sassy": "^1.2.0",
    "wintersmith-livereload": "^0.2.2"
  },
  "devDependencies": {
  }
}
